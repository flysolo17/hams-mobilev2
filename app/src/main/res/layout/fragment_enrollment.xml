<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EDEAEA"

    tools:context=".views.main.nav.enrollment.EnrollmentFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerviewEnrollments"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:listitem="@layout/list_enrollment" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/buttonEnroll"
        style="@style/Widget.Material3.ExtendedFloatingActionButton.Icon.Primary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="16dp"
        android:text="@string/enroll_now"
        app:icon="@drawable/baseline_add_circle_24"
        app:iconPadding="10dp" />
</FrameLayout>